{
  "name": "sdd-with-claude-code-template",
  "version": "0.1.0",
  "description": "SDD with Claude Code Template",
  "type": "module",
  "scripts": {
    "prepare": "husky",
    "build": "npm run build:server && npm run build:client",
    "build:server": "tsc -p tsconfig.server.json",
    "build:client": "vite build",
    "dev": "tsc -p tsconfig.server.json --watch",
    "dev:server": "tsc -p tsconfig.server.json --watch",
    "dev:client": "vite",
    "lint": "eslint .",
    "format": "npx prettier --write .",
    "typecheck": "vue-tsc --noEmit -p tsconfig.app.json && tsc --noEmit -p tsconfig.server.json",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui"
  },
  "keywords": [
    "spec-driven-development",
    "claude-code",
    "template"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/fluent-ffmpeg": "^2.1.28",
    "@types/node": "^20.11.0",
    "@types/uuid": "^10.0.0",
    "@vitejs/plugin-vue": "^6.0.3",
    "@vitest/coverage-v8": "^2.0.0",
    "@vitest/ui": "^2.0.0",
    "eslint": "^9.0.0",
    "eslint-config-prettier": "^9.1.0",
    "husky": "^9.0.0",
    "lint-staged": "^15.2.0",
    "prettier": "^3.2.0",
    "typescript": "~5.3.0",
    "typescript-eslint": "^8.0.0",
    "vite": "^5.4.21",
    "vitest": "^2.0.0",
    "vue-tsc": "^3.2.4"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "dependencies": {
    "@hono/node-server": "^1.19.9",
    "dotenv": "^17.2.3",
    "fluent-ffmpeg": "^2.1.3",
    "hono": "^4.11.7",
    "replicate": "^1.4.0",
    "uuid": "^13.0.0",
    "vue": "^3.5.27"
  }
}
